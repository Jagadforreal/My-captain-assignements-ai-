    "for i in x :\n",
    "    if (i > 0):\n",
    "        print(i,end=' ')#output1\n",
    "        \n",
    "print('\\n')        \n",
    "\n",
    "z=[]        \n",
    "for j in y:\n",
    "    if (j > 0):\n",
    "        z.append(j)\n",
    "        \n",
    "print (z)#output2\n"
